<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ladyjek.twiscode.com.ladyjek" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name=".Activity.ActivitySplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.ActivityLogin"
            android:label="@string/title_activity_activity_login"
            android:windowSoftInputMode="adjustResize">
        </activity>
        <activity
            android:name=".Activity.ActivityRegister"
            android:label="@string/title_activity_activity_register"
            android:windowSoftInputMode="adjustResize">
        </activity>
        <activity
            android:name=".Activity.ActivityConfirm"
            android:label="@string/title_activity_activity_confirm"
            android:windowSoftInputMode="adjustResize">
        </activity>
        <activity
            android:name=".Activity.ActivityRate"
            android:label="@string/title_activity_activity_rate"
            android:windowSoftInputMode="adjustResize">
        </activity>
    </application>

</manifest>
